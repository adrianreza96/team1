@model IEnumerable<FeastFreedom.Models.User>
@{
    Layout = null;
}

@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width-device-width" />
    <title>Index</title>
</head>
<body>
    <h2>@ViewBag.UserList</h2>
    <div>
        <table>
            <tr>
                <th>UserId</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Role Id</th>
            </tr>
            @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(model => item.UserId) </td>
            <td>@Html.DisplayFor(model => item.FirstName) </td>
            <td>@Html.DisplayFor(model => item.LastName) </td>
            <td>@Html.DisplayFor(model => item.RoleId) </td>
        </tr>
            }

        </table>
    </div>
</body>
</html>
